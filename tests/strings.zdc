
#dynamic_link "libc.so.6";

#foreign putchar :: (c: int) -> int;
#foreign getchar :: () -> int;
#foreign printf :: () -> void;

newline :: () -> void { putchar(10); }

print_digit :: (digit: int) -> void
{
    putchar(digit + 48);
}

main :: () -> int
{
    putchar(50);
    newline();
    print_digit(7);
    newline();
    c1 := getchar();
    putchar(c1);
    newline();
    return getchar();
}
