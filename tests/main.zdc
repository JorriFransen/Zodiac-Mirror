sub :: (a: int, b: int)
{
    return a - b;
}

fib_recursive :: (x: int) -> int
{
    if (x < 2)
    {
        return x;
    }
    else
    {
        return fib_recursive(x - 1) + fib_recursive(x - 2);
    }
}

fact_recursive :: (n: int) -> int
{
    if (n <= 1)
    {
        return 1;
    }
    else
    {
        return n * fact_recursive(n - 1);
    }
}

