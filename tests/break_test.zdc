
io :: import io;

main :: () -> void
{
    for (i := 0; i < 20; i = i + 1)
    {
        io.printf("i: %ld\n", i);

        if (i >= 9)
            break;
    }

    io.printf("\n");

    j := 20;
    while (true)
    {
        io.printf("j: %ld\n", j);

        if (j <= 10)
            break;

        j = j - 1;
    }

}
