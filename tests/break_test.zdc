
c :: import c;

main :: () -> void
{
    for (i := 0; i < 20; i += 1)
    {
        c.printf("i: %ld\n", i);

        if (i >= 9)
            break;
    }

    c.printf("\n");

    j := 20;
    while (true)
    {
        c.printf("j: %ld\n", j);

        if (j <= 10)
            break;

        j -= 1;
    }

}
