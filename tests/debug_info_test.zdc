
_c :: import c;

int :: typedef s64;

Vec2 :: struct
{
    x: int;
    y: int;
}

Vec3 :: struct
{
    xy : Vec2;
    z  : float;
}

main :: () -> int
{

    v : Vec2 = {1, 2};
    v2 : Vec2 = { 3, 4 };
    v3 : Vec3 = { {5, 6}, 7.8 };

    _c.printf("v : { %d, %d }\n", v.x,  v.y);
    _c.printf("v2: { %d, %d }\n", v2.x,  v2.y);
    _c.printf("v3: { { %d, %d }, %f }\n", v3.xy.x, v3.xy.y, v3.z);

    return 0;
}
