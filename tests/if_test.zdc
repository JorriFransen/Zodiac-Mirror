
io :: import io;

main :: () -> void
{
    c := io.getchar();
    if (c == 49)
    {
       io.printf("You choose number one\n");
    }
    else if (c == 50)
    {
       io.printf("You choose number two\n");
    }
    else if (c == 51)
    {
        io.printf("You choose number three\n");
    }
    else if (c == 52)
    {
        io.printf("you choose number four\n");
    }
    else
    {
        io.printf("Invalid option\n");
    }
}
